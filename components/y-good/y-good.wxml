<!--components/y-good/y-good.wxml-->
<view class="good">
    <block wx:for="{{good}}" wx:key="index">
        <y-good-item item="{{item}}"
                     class="good-item"
                     bindtap="goodItemClick"
                     data-iid="{{item.iid}}"></y-good-item>
    </block>
</view>
