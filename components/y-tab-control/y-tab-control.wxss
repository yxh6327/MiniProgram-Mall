/* components/tabcontrol/TabControl.wxss */
.tab-control{
    display: flex;
    align-items: center;
    height: 88rpx;
    font-size: 32rpx;
}
.tab-control-item{
    flex: 1;
    text-align: center;
    margin: 0 40rpx;
    padding-bottom: 16rpx;
}
.active{
    border-bottom: 6rpx solid #ff5777;
    color: #ff5777;
}